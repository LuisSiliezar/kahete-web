---
import HeaderLink from './HeaderLink.astro';
import { SITE_TITLE } from '../consts';
---
<header>
    <nav class="bg-slate-950px-4 lg:px-6 py-2.5 dark:bg-slate-950">
        <div class="flex flex-wrap justify-between items-stretch mx-auto max-w-screen-xl">
            <a class="flex items-center">
                <img src="../Logo.svg" class="mr-3 h-6 sm:h-9" alt="Flowbite Logo" />
                <span class="self-center text-xl font-semibold whitespace-nowrap dark:text-white">Kahete</span>
            </a>
            <div class="flex items-center">
                <a href="#" class="text-white ">Get started</a>
            </div>
        </div>
    </nav>
</header>
<style>
	.social-links,
	.social-links a {
		/* display: flex; */
	}
	@media (max-width: 720px) {
		.social-links {
			display: none;
		}
	}
</style>
